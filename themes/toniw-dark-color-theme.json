{
  "name": "Toniw Dark",
  "type": "dark",
  "colors": {
    // TODO: log type colors/bold-style
    "activityBar.background": "#323447", // ACTIVITY BG
    "activityBar.border": "#1b1c26", // UI BORDER
    "activityBar.foreground": "#f0f0f0", // FG
    "activityBarBadge.background": "#007acc",
    "badge.background": "#565a7a",
    "debugToolBar.background": "#2d2f40", // EDITOR BG
    "diffEditor.insertedLineBackground": "#39aa1730",
    "diffEditor.insertedTextBackground": "#39aa1730",
    "diffEditor.removedLineBackground": "#ff202030",
    "diffEditor.removedTextBackground": "#ff202030",
    "dropdown.background": "#21232f", // INPUT BG
    "dropdown.border": "#1b1c26", // UI BORDER
    "editor.background": "#2d2f40", // EDITOR BG
    "editor.foreground": "#f0f0f0", // FG
    "editor.lineHighlightBackground": "#373a4d", // LINE HIGHLIGHT BG
    "editor.selectionBackground": "#585a7e",
    "editor.selectionHighlightBackground": "#585a7e90",
    "editorBracketMatch.background": "#282938", // SIDEBAR BG
    "editorBracketMatch.border": "#878baa",
    "editorGroup.border": "#1b1c26", // UI BORDER
    "editorGroupHeader.tabsBackground": "#282938", // SIDEBAR BG
    "editorGutter.addedBackground": "#3d8f2d",
    "editorGutter.deletedBackground": "#cd4a4a",
    "editorGutter.modifiedBackground": "#50a5e7",
    "editorIndentGuide.activeBackground1": "#6c6f97",
    "editorIndentGuide.background1": "#4c4e6b",
    "editorLineNumber.activeForeground": "#d3d5e6",
    "editorLineNumber.foreground": "#5f6381",
    "editorMarkerNavigation.background": "#282938", // SIDEBAR BG
    "editorOverviewRuler.addedForeground": "#3d8f2db0", // ADDED (DIMMED)
    "editorOverviewRuler.border": "#00000000", // NO BORDER
    "editorOverviewRuler.deletedForeground": "#cd4a4ab0", // DELETED (DIMMED)
    "editorOverviewRuler.modifiedForeground": "#50a5e7b0", // MODIFIED (DIMMED)
    "editorRuler.foreground": "#4c4e6b",
    "editorWidget.background": "#282938", // SIDEBAR BG
    "editorWidget.border": "#1b1c26", // UI BORDER
    "foreground": "#f0f0f0", // FG
    "input.background": "#21232f", // INPUT BG
    "input.border": "#1b1c26", // UI BORDER
    "list.activeSelectionBackground": "#3d3f56",
    "list.focusBackground": "#54587780",
    "list.hoverBackground": "#3f425980",
    "list.inactiveSelectionBackground": "#3d3f56",
    "panel.border": "#1b1c26", // UI BORDER
    "panelTitle.activeBorder": "#565a7a",
    "peekViewTitle.background": "#21232f", // INPUT BG
    "scrollbar.shadow": "#00000050",
    "scrollbarSlider.activeBackground": "#696e9580",
    "scrollbarSlider.background": "#54587780",
    "scrollbarSlider.hoverBackground": "#5f638680",
    "sideBar.background": "#282938", // SIDEBAR BG
    "sideBar.border": "#1b1c26", // UI BORDER
    "sideBar.foreground": "#e0e0e0", // SIDEBAR FG 
    "sideBarSectionHeader.background": "#323447", // ACTIVITY BG
    "sideBarTitle.foreground": "#ffffffb0", // FG DIMMED
    "statusBar.background": "#282938", // SIDEBAR BG
    "statusBar.border": "#1b1c26", // UI BORDER
    "statusBar.debuggingBackground": "#282938", // SIDEBAR BG
    "statusBar.foreground": "#ffffffb0", // FG DIMMED
    "statusBar.noFolderBackground": "#282938", // SIDEBAR BG
    "statusBarItem.remoteBackground": "#323447", // ACTIVITY BG
    "tab.activeBackground": "#323447", // TAB BG
    "tab.activeBorder": "#6b74bb", // TAB BORDER
    "tab.border": "#00000000",
    "tab.inactiveBackground": "#282938", // SIDEBAR BG
    "titleBar.activeBackground": "#323447", // ACTIVITY BG
    "titleBar.inactiveBackground": "#323447", // ACTIVITY BG
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#728aa7" // COMMENTS
      }
    },
    {
      "name": "Doc Annotations",
      "scope": [
        "storage.type.annotation",
        "comment storage.type.class",
        "comment keyword.other.documentation",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#a0bbde" // ANNOTATIONS
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#e5d7a5" // LOCAL VARIABLES
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ff5353" // ERRORS
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.other",
        "storage.type",
        "storage.modifier",
        "constant.language",
        "variable.language", // "this"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#db7bd4" // KEYWORDS
      }
    },
    {
      "name": "Import Names",
      "scope": [
        "storage.modifier.import",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#f0f0f0" // DEFAULT
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "punctuation",
        "meta.brace", // all other braces []()
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#bbecff" // PUNCTUATIONS
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#db7bd4" // TAG -> KEYWORDS
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f0f0f0" // FUNCTIONS
      }
    },
    {
      "name": "CSS",
      "scope": [
        "source.css support.function"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#5cc7cc" // CSS FUNCTION -> ENUMS
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#e5d7a5" // MARKDOWN ALT TEXT -> VARIABLES
      }
    },
    {
      "name": "Constant, Tag Attribute, Embedded",
      "scope": [
        "support.constant",
        "constant.character", // e.g. "&nbsp;"
        "constant.escape",
      ],
      "settings": {
        "foreground": "#5cc7cc" // ENUMS
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.other.color", // hex colors
        "constant.other.rgb-value", // hex colors
        "keyword.other.unit",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ff9259" // NUMBERS
      }
    },
    {
      "name": "Function Argument",
      "scope": [
        "variable.parameter",
      ],
      "settings": {
        "foreground": "#e6a993" // PARAMETER VARIABLES
      }
    },
    {
      "name": "String, Symbols, Inherited Class",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#97d982" // STRINGS
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "entity.other.inherited-class",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "support.type.sys-types"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#d8ccff" // CLASSES
      }
    },
    {
      "name": "New Class Instance",
      "scope": [
        "new.expr entity.name.type",
      ],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#e5d7a5" // CSS PROPERTY -> VARIABLES
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "variable.other.property",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#8cb2ff" // FIELDS 
      }
    },
    {
      "name": "CSS Classes, IDs",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        "source.sass keyword.control"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#d8ccff" // CSS CLASS, ID -> CLASSES
      }
    },
    {
      "name": "Inserted (Markup, Patch)",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#97d982" // INSERTED -> STRINGS
      }
    },
    {
      "name": "Deleted (Markup, Patch)",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#ff5353" // DELETED -> ERRORS
      }
    },
    {
      "name": "Changed (Markup, Patch)",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#2075b6" // CHANGED
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#89ddff" // REGEX -> PUNCTUATION
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#89ddff" // ESC -> PUNCTUATION
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline" // URLS
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json",
        "meta.object-literal.key",
      ],
      "settings": {
        "foreground": "#e5d7a5" // JSON KEY -> VARIABLES
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#f0f0f0" // DEFAULT
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#db7bd4" // HEADING -> KEYWORDS
      }
    },
    {
      "name": "Markdown - Heading (all bold)",
      "scope": [
        "markup.heading entity.name",
      ],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "name": "Markup - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5cc7cc" // STYLE -> ENUMS
      }
    },
    {
      "name": "Markup - Bold",
      "scope": [
        "markup.bold",
        "markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#5cc7cc" // STYLE -> ENUMS
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#5cc7cc" // STYLE -> ENUMS
      }
    },
    {
      "name": "Markup - Underline",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#97d982" // LINK URL -> STRINGS
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": [
        "markup.quote punctuation.definition.blockquote.markdown"
      ],
      "settings": {
        "foreground": "#bbecff" // BLOCKQUOTE -> PUNCTUATIONS
      }
    },
    {
      "name": "Markup - Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "fontStyle": "italic",
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "string.other.link.title.markdown"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#e5d7a5" // LINK LABEL -> VARIABLES
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": [
        "string.other.link.description.title.markdown"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#e5d7a5" // LINK TITLE -> VARIABLES
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": [
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#8cb2ff" // ID ANCHOR -> FIELDS
      }
    },
    {
      "name": "Markup - Raw",
      "scope": [
        "markup.raw.block",
        "markup.inline.raw.string.markdown",
        "text.html.markdown markup.inline.raw.markdown"
      ],
      "settings": {
        "foreground": "#d8ccff" // RAW / CODE -> CLASSES
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": [
        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f0f0f080" // SEPARATORS -> DEFAULT (DIMMED)
      }
    },
    {
      "name": "Markup - Table",
      "scope": [
        "markup.table"
      ],
      "settings": {
        "foreground": "#bbecff" // TABLE -> PUNCTUATIONS
      }
    }
  ]
}
