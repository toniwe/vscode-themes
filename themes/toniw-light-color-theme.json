  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6a8ebd" // COMMENTS
      }
    },
    {
      "name": "Doc Annotations",
      "scope": [
        "storage.type.annotation",
        "comment storage.type.class",
        "comment keyword.other.documentation",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#264b80" // ANNOTATIONS
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#705800" // LOCAL VARIABLES
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#db1c3c" // ERRORS
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.other",
        "keyword.operator",
        "punctuation.separator.regex.alternation",
        "punctuation.definition.regex.alternation",
        "storage.type",
        "storage.modifier",
        "constant.language",
        "variable.language", // "this"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bd22b2" // KEYWORDS
      }
    },
    {
      "name": "Import Names",
      "scope": [
        "storage.modifier.import",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#101010" // DEFAULT
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "punctuation",
        "meta.brace", // all other braces []()
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#006087" // PUNCTUATIONS
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bd22b2" // TAG -> KEYWORDS
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#101010" // FUNCTIONS
      }
    },
    {
      "name": "CSS",
      "scope": [
        "source.css support.function"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#007377" // CSS FUNCTION -> ENUMS
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#705800" // MARKDOWN ALT TEXT -> VARIABLES
      }
    },
    {
      "name": "Constant, Tag Attribute, Embedded",
      "scope": [
        "support.constant",
        "constant.character", // e.g. "&nbsp;"
        "constant.escape",
      ],
      "settings": {
        "foreground": "#007377" // ENUMS
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.other.color", // hex colors
        "constant.other.rgb-value", // hex colors
        "keyword.other.unit",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#cc4400" // NUMBERS
      }
    },
    {
      "name": "Function Argument",
      "scope": [
        "variable.parameter",
      ],
      "settings": {
        "foreground": "#661808" // PARAMETER VARIABLES
      }
    },
    {
      "name": "String, Symbols, Inherited Class",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#1c7800" // STRINGS
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "entity.other.inherited-class",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "support.type.sys-types"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2c1185" // CLASSES
      }
    },
    {
      "name": "New Class Instance",
      "scope": [
        "new.expr entity.name.type",
      ],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#705800" // CSS PROPERTY -> VARIABLES
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "variable.other.property",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#094be6" // FIELDS 
      }
    },
    {
      "name": "CSS Classes, IDs",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        "source.sass keyword.control"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2c1185" // CSS CLASS, ID -> CLASSES
      }
    },
    {
      "name": "Inserted (Markup, Patch)",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#1c7800" // INSERTED -> STRINGS
      }
    },
    {
      "name": "Deleted (Markup, Patch)",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#db1c3c" // DELETED -> ERRORS
      }
    },
    {
      "name": "Changed (Markup, Patch)",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#1c8bdf" // CHANGED
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#006087" // REGEX -> PUNCTUATIONS
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#006087" // ESC -> PUNCTUATIONS
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline" // URLS
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json",
        "meta.object-literal.key",
      ],
      "settings": {
        "foreground": "#705800" // JSON KEY -> VARIABLES
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#101010" // DEFAULT
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bd22b2" // HEADING -> KEYWORDS
      }
    },
    {
      "name": "Markdown - Heading (all bold)",
      "scope": [
        "markup.heading entity.name",
      ],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "name": "Markup - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#007377" // STYLE -> ENUMS
      }
    },
    {
      "name": "Markup - Bold",
      "scope": [
        "markup.bold",
        "markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#007377" // STYLE -> ENUMS
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#007377" // STYLE -> ENUMS
      }
    },
    {
      "name": "Markup - Underline",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#1c7800" // LINK URL -> STRINGS
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": [
        "markup.quote punctuation.definition.blockquote.markdown"
      ],
      "settings": {
        "foreground": "#006087" // BLOCKQUOTE -> PUNCTUATIONS
      }
    },
    {
      "name": "Markup - Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "fontStyle": "italic",
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "string.other.link.title.markdown"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#705800" // LINK LABEL -> VARIABLES
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": [
        "string.other.link.description.title.markdown"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#705800" // LINK TITLE -> VARIABLES
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": [
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#094be6" // ID ANCHOR -> FIELDS
      }
    },
    {
      "name": "Markup - Raw",
      "scope": [
        "markup.raw.block",
        "markup.inline.raw.string.markdown",
        "text.html.markdown markup.inline.raw.markdown"
      ],
      "settings": {
        "foreground": "#2c1185" // RAW / CODE -> CLASSES
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": [
        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#10101080" // SEPARATORS -> DEFAULT (DIMMED)
      }
    },
    {
      "name": "Markup - Table",
      "scope": [
        "markup.table"
      ],
      "settings": {
        "foreground": "#006087" // TABLE -> PUNCTUATIONS
      }
    }
  ]
}
