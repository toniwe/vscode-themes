  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#4D6E99" // COMMENTS
      }
    },
    {
      "name": "Doc Annotations",
      "scope": [
        "storage.type.annotation",
        "comment storage.type.class",
        "comment keyword.other.documentation",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#264B80" // ANNOTATIONS
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#806A00" // LOCAL VARIABLES
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#db1c3c" // ERRORS
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.other",
        "storage.type",
        "storage.modifier",
        "constant.language",
        "variable.language", // "this"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ce4bc5" // KEYWORDS
      }
    },
    {
      "name": "Import Names",
      "scope": [
        "storage.modifier.import",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#202020" // DEFAULT
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "punctuation",
        "meta.brace", // all other braces []()
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#00597D" // PUNCTUATIONS
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ce4bc5" // TAG -> KEYWORDS
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#202020" // FUNCTIONS
      }
    },
    {
      "name": "CSS",
      "scope": [
        "source.css support.function"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#1C7D81" // CSS FUNCTION -> ENUMS
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#806A00" // MARKDOWN ALT TEXT -> VARIABLES
      }
    },
    {
      "name": "Constant, Tag Attribute, Embedded",
      "scope": [
        "support.constant",
        "constant.character", // e.g. "&nbsp;"
        "constant.escape",
      ],
      "settings": {
        "foreground": "#1C7D81" // ENUMS
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.other.color", // hex colors
        "constant.other.rgb-value", // hex colors
        "keyword.other.unit",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#CC4400" // NUMBERS
      }
    },
    {
      "name": "Function Argument",
      "scope": [
        "variable.parameter",
      ],
      "settings": {
        "foreground": "#7c1f00" // PARAMETER VARIABLES
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#2D7217" // STRINGS
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "entity.other.inherited-class",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#301195" // CLASSES
      }
    },
    {
      "name": "New Class Instance",
      "scope": [
        "new.expr entity.name.type",
      ],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#806A00" // CSS PROPERTY -> VARIABLES
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "variable.other.property",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#094dd4" // FIELDS 
      }
    },
    {
      "name": "CSS Classes, IDs",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        "source.sass keyword.control"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#301195" // CSS CLASS, ID -> CLASSES
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#97D982" // ???
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#FF5370" // ???
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#C792EA" // ???
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#00597D" // REGEX -> PUNCTUATIONS
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#00597D" // ESC -> PUNCTUATIONS
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline" // URLS
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json",
        "meta.object-literal.key",
      ],
      "settings": {
        "foreground": "#806A00" // JSON KEY -> VARIABLES
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#202020" // DEFAULT
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ce4bc5" // HEADING -> KEYWORDS
      }
    },
    {
      "name": "Markdown - Heading (all bold)",
      "scope": [
        "markup.heading entity.name",
      ],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "name": "Markup - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#1C7D81" // STYLE -> ENUM
      }
    },
    {
      "name": "Markup - Bold",
      "scope": [
        "markup.bold",
        "markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#1C7D81" // STYLE -> ENUM
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#1C7D81" // STYLE -> ENUM
      }
    },
    {
      "name": "Markup - Underline",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#2D7217" // LINK URL -> STRINGS
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": [
        "markup.quote punctuation.definition.blockquote.markdown"
      ],
      "settings": {
        "foreground": "#00597D" // BLOCKQUOTE -> PUNCTUATIONS
      }
    },
    {
      "name": "Markup - Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "fontStyle": "italic",
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "string.other.link.title.markdown"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#806A00" // LINK LABEL -> VARIABLES
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": [
        "string.other.link.description.title.markdown"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#806A00" // LINK TITLE -> VARIABLES
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": [
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#094dd4" // ID ANCHOR -> FIELDS
      }
    },
    {
      "name": "Markup - Raw",
      "scope": [
        "markup.raw.block",
        "markup.inline.raw.string.markdown",
        "text.html.markdown markup.inline.raw.markdown"
      ],
      "settings": {
        "foreground": "#301195" // RAW / CODE -> CLASSES
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": [
        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#20202080" // SEPARATORS -> DEFAULT (DIMMED)
      }
    },
    {
      "name": "Markup - Table",
      "scope": [
        "markup.table"
      ],
      "settings": {
        "foreground": "#00597D" // TABLE -> PUNCTUATIONS
      }
    }
  ]
}
